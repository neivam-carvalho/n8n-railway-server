# n8n Configuration
N8N_HOST=0.0.0.0
N8N_PORT=5678
N8N_PROTOCOL=https
N8N_EDITOR_BASE_URL=https://your-app-name.up.railway.app

# Database Configuration
DB_TYPE=postgresdb
DB_POSTGRESDB_HOST=your-postgres-host
DB_POSTGRESDB_PORT=5432
DB_POSTGRESDB_DATABASE=your-database-name
DB_POSTGRESDB_USER=your-database-user
DB_POSTGRESDB_PASSWORD=your-database-password
DB_POSTGRESDB_SCHEMA=public
DB_POSTGRESDB_SSL_ENABLED=true
DB_POSTGRESDB_SSL_REJECT_UNAUTHORIZED=false

# Redis Configuration (for queue mode)
QUEUE_BULL_REDIS_HOST=your-redis-host
QUEUE_BULL_REDIS_PORT=6379
QUEUE_BULL_REDIS_PASSWORD=your-redis-password
EXECUTIONS_MODE=queue

# Security
N8N_ENCRYPTION_KEY=your-encryption-key-min-10-chars
N8N_USER_MANAGEMENT_JWT_SECRET=your-jwt-secret-min-32-chars

# Authentication (opcional)
N8N_BASIC_AUTH_ACTIVE=false
N8N_BASIC_AUTH_USER=admin
N8N_BASIC_AUTH_PASSWORD=your-password

# Webhook Configuration
WEBHOOK_URL=https://your-app-name.up.railway.app

# Node Environment
NODE_ENV=production

# Timezone
GENERIC_TIMEZONE=America/Sao_Paulo

# File Storage
N8N_DEFAULT_BINARY_DATA_MODE=filesystem
N8N_BINARY_DATA_TTL=24

# Metrics (opcional)
N8N_METRICS=false

# Log Level
N8N_LOG_LEVEL=info

# Disable version notifications
N8N_VERSION_NOTIFICATIONS_ENABLED=false

# Executions Data Prune
EXECUTIONS_DATA_PRUNE=true
EXECUTIONS_DATA_MAX_AGE=336